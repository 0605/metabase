                                            "nullable": false,
                                            "references": "setting(key)",
                                            "foreignKeyName": "fk_setting_value_ref_setting_key",
                                            "deferrable": false,
                                            "initiallyDeferred": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "value",
                                        "type": "varchar(254)",
                                        "constraints": {
                                            "nullable": false
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "addUniqueConstraint": {
                            "tableName": "setting_value",
                            "constraintName": "idx_unique_setting_id_organization_id",
