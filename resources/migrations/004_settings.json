{
    "databaseChangeLog": [
        {
            "changeSet": {
                "id": "4",
                "author": "cammsaul",
                "changes": [
                    {
                        "createTable": {
                            "tableName": "setting",
                            "columns": [
                                {
                                    "column": {
                                        "name": "key",
                                        "type": "varchar(254)",
                                        "constraints": {
                                            "primaryKey": true,
                                            "nullable": false
                                        }
                                    }
                                },
                                {
                                    "column": {
                                        "name": "description",
                                        "type": "varchar(254)"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "createTable": {
                            "tableName": "setting_value",
                            "columns": [
                                {
                                    "column": {
                                        "name": "id",
                                        "type": "int",
                                        "autoIncrement": true,
                                        "constraints": {
                                            "primaryKey": true,
                                            "nullable": false
                                        }
                                    }
